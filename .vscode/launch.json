	"version": "0.2.0",
	"configurations": [
	{
		"name": "(gdb) Launch",
		"type": "cppdbg",
		"request": "launch",
		"program": "${workspaceFolder}/hello_world.exe",
		"args": [],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": "false",
		"MIMode": "gdb",
		"miDebuggerPath": "c:\\MinGW\\bin\\gdb.exe",
		"setupCommands": [
			{
				"description": "Enable pretty-printing for gdb",
				"text": "-enable-pretty-printing",
				"ignoreFailures": false
			}
		]
	}
	]
}
